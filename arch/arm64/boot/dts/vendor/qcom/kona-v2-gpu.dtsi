&soc {
	msm_bus: qcom,kgsl-busmon {
		label = "kgsl-busmon";
		compatible = "qcom,kgsl-busmon";
		operating-points-v2 = <&gpu_opp_table_v2>;
	};

	gpu_opp_table_v2: gpu-opp-table_v2 {
		compatible = "operating-points-v2";
		
		opp-903000000 {
			opp-hz = /bits/ 64 <903000000>;
			opp-microvolt = <RPMH_REGULATOR_LEVEL_TURBO_L1>;
		};
		
		opp-840000000 {
			opp-hz = /bits/ 64 <840000000>;
			opp-microvolt = <RPMH_REGULATOR_LEVEL_NOM_L1P>;
		};

		opp-719000000 {
			opp-hz = /bits/ 64 <719000000>;
			opp-microvolt = <RPMH_REGULATOR_LEVEL_NOM_L1>;
		};

		opp-680000000 {
			opp-hz = /bits/ 64 <680000000>;
			opp-microvolt = <RPMH_REGULATOR_LEVEL_NOM_L0>;
		};

		opp-615000000 {
			opp-hz = /bits/ 64 <615000000>;
			opp-microvolt = <RPMH_REGULATOR_LEVEL_NOM>;
		};

		opp-587000000 {
			opp-hz = /bits/ 64 <587000000>;
			opp-microvolt = <RPMH_REGULATOR_LEVEL_SVS_L2>;
		};

		opp-550000000 {
			opp-hz = /bits/ 64 <550000000>;
			opp-microvolt = <RPMH_REGULATOR_LEVEL_SVS_L1P>;
		};

		opp-435000000 {
			opp-hz = /bits/ 64 <435000000>;
			opp-microvolt = <RPMH_REGULATOR_LEVEL_MIN_SVS_L1>;
		};

	};
};

&msm_gpu {
	qcom,chipid = <0x06050001>;

	/* GPU OPP data */
	operating-points-v2 = <&gpu_opp_table_v2>;

	/delete-property/qcom,initial-pwrlevel;
	/delete-node/qcom,gpu-pwrlevels;

	/* Power levels bins */
	qcom,gpu-pwrlevel-bins {
		compatible="qcom,gpu-pwrlevel-bins";
		#address-cells = <1>;
		#size-cells = <0>;

		qcom,gpu-pwrlevels-0 {
			#address-cells = <1>;
			#size-cells = <0>;
			qcom,speed-bin = <0>;
			qcom,initial-pwrlevel = <8>;
			qcom,throttle-pwrlevel = <0>;

			qcom,gpu-pwrlevel@0 {
				reg = <0>;
				qcom,gpu-freq = <903000000>;

				qcom,bus-freq-ddr7 = <12>;
				qcom,bus-freq-ddr8 = <12>;

				qcom,acd-level = <0x882d5ffd>;
			};

			qcom,gpu-pwrlevel@1 {
				reg = <1>;
				qcom,gpu-freq = <840000000>;

				qcom,bus-freq-ddr7 = <12>;
				qcom,bus-freq-ddr8 = <12>;

				qcom,acd-level = <0x882d5ffd>;
			};

			qcom,gpu-pwrlevel@2 {
				reg = <2>;
				qcom,gpu-freq = <719000000>;

				qcom,bus-freq-ddr7 = <12>;
				qcom,bus-freq-ddr8 = <12>;

				qcom,acd-level = <0x882e5ffd>;
			};

			qcom,gpu-pwrlevel@3 {
				reg = <3>;
				qcom,gpu-freq = <680000000>;

				qcom,bus-freq-ddr7 = <12>;
				qcom,bus-freq-ddr8 = <11>;

				qcom,acd-level = <0x882e5ffd>;
			};

			qcom,gpu-pwrlevel@4 {
				reg = <4>;
				qcom,gpu-freq = <615000000>;

				qcom,bus-freq-ddr7 = <11>;
				qcom,bus-freq-ddr8 = <11>;

				qcom,acd-level = <0xa82f5ffd>;
			};

			qcom,gpu-pwrlevel@5 {
				reg = <5>;
				qcom,gpu-freq = <587000000>;

				qcom,bus-freq-ddr7 = <11>;
				qcom,bus-freq-ddr8 = <11>;

				qcom,acd-level = <0xa82f5ffd>;
			};

			qcom,gpu-pwrlevel@6 {
				reg = <6>;
				qcom,gpu-freq = <550000000>;

				qcom,bus-freq-ddr7 = <10>;
				qcom,bus-freq-ddr8 = <9>;

				qcom,acd-level = <0xa82f5ffd>;
			};

			qcom,gpu-pwrlevel@7 {
				reg = <7>;
				qcom,gpu-freq = <435000000>;

				qcom,bus-freq-ddr7 = <8>;
				qcom,bus-freq-ddr8 = <7>;

				qcom,acd-level = <0xa82f5ffd>;
			};

			qcom,gpu-pwrlevel@8 {
				reg = <8>;
				qcom,gpu-freq = <0>;
				qcom,bus-freq = <0>;
				qcom,bus-min = <0>;
				qcom,bus-max = <0>;
			};
		};
	};
};
