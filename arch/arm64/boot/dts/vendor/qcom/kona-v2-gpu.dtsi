&soc {
	msm_bus: qcom,kgsl-busmon {
		label = "kgsl-busmon";
		compatible = "qcom,kgsl-busmon";
		operating-points-v2 = <&gpu_opp_table_v2>;
	};

	gpu_opp_table_v2: gpu-opp-table_v2 {
		compatible = "operating-points-v2";
		
		opp-917988888 {
			opp-hz = /bits/ 64 <917988888>;
			opp-microvolt = <RPMH_REGULATOR_LEVEL_TURBO_L2>;
		};
		
		opp-733333333 {
			opp-hz = /bits/ 64 <733333333>;
			opp-microvolt = <RPMH_REGULATOR_LEVEL_NOM_L2D>;
		};

		opp-682978000 {
			opp-hz = /bits/ 64 <682978000>;
			opp-microvolt = <RPMH_REGULATOR_LEVEL_NOM_L1P>;
		};

		opp-654000000 {
			opp-hz = /bits/ 64 <654000000>;
			opp-microvolt = <RPMH_REGULATOR_LEVEL_NOM_L1D>;
		};

		opp-635000000 {
			opp-hz = /bits/ 64 <635000000>;
			opp-microvolt = <RPMH_REGULATOR_LEVEL_NOM>;
		};

		opp-613000000 {
			opp-hz = /bits/ 64 <613000000>;
			opp-microvolt = <RPMH_REGULATOR_LEVEL_SVS_L2>;
		};

		opp-511900000 {
			opp-hz = /bits/ 64 <511900000>;
			opp-microvolt = <RPMH_REGULATOR_LEVEL_LOW_SVS_L2>;
		};

		opp-411111111 {
			opp-hz = /bits/ 64 <411111111>;
			opp-microvolt = <RPMH_REGULATOR_LEVEL_MIN_SVS>;
		};

	};
};

&msm_gpu {
	qcom,chipid = <0x06050001>;

	/* GPU OPP data */
	operating-points-v2 = <&gpu_opp_table_v2>;

	/delete-property/qcom,initial-pwrlevel;
	/delete-node/qcom,gpu-pwrlevels;

	/* Power levels bins */
	qcom,gpu-pwrlevel-bins {
		compatible="qcom,gpu-pwrlevel-bins";
		#address-cells = <1>;
		#size-cells = <0>;

		qcom,gpu-pwrlevels-0 {
			#address-cells = <1>;
			#size-cells = <0>;
			qcom,speed-bin = <0>;
			qcom,initial-pwrlevel = <7>;
			qcom,throttle-pwrlevel = <1>;

			qcom,gpu-pwrlevel@0 {
				reg = <0>;
				qcom,gpu-freq = <917988888>;
				
				qcom,bus-freq-ddr7 = <12>;
				qcom,bus-freq-ddr8 = <11>;

				qcom,acd-level = <0x802b5ffd>;
			};

			qcom,gpu-pwrlevel@1 {
				reg = <1>;
				qcom,gpu-freq = <733333333>;
				
				qcom,bus-freq-ddr7 = <12>;
				qcom,bus-freq-ddr8 = <11>;

				qcom,acd-level = <0x802b5ffd>;
			};

			qcom,gpu-pwrlevel@2 {
				reg = <2>;
				qcom,gpu-freq = <682978000>;
				
				qcom,bus-freq-ddr7 = <12>;
				qcom,bus-freq-ddr8 = <11>;

				qcom,acd-level = <0x802b5ffd>;
			};

			qcom,gpu-pwrlevel@3 {
				reg = <3>;
				qcom,gpu-freq = <654000000>;
				
				qcom,bus-freq-ddr7 = <12>;
				qcom,bus-freq-ddr8 = <11>;

				qcom,acd-level = <0x802b5ffd>;
			};

			qcom,gpu-pwrlevel@4 {
				reg = <4>;
				qcom,gpu-freq = <635000000>;
				
				qcom,bus-freq-ddr7 = <12>;
				qcom,bus-freq-ddr8 = <11>;

				qcom,acd-level = <0x802b5ffd>;
			};

			qcom,gpu-pwrlevel@5 {
				reg = <5>;
				qcom,gpu-freq = <613000000>;
				
				qcom,bus-freq-ddr7 = <12>;
				qcom,bus-freq-ddr8 = <11>;

				qcom,acd-level = <0x802b5ffd>;
			};

			qcom,gpu-pwrlevel@6 {
				reg = <6>;
				qcom,gpu-freq = <511900000>;

				qcom,bus-freq-ddr7 = <5>;
				qcom,bus-freq-ddr8 = <5>;

				qcom,acd-level = <0x802b5ffd>;
			};

			qcom,gpu-pwrlevel@7 {
				reg = <7>;
				qcom,gpu-freq = <411111111>;

				qcom,bus-freq-ddr7 = <1>;
				qcom,bus-freq-ddr8 = <1>;

				qcom,acd-level = <0x802b5ffd>;
			};

			qcom,gpu-pwrlevel@8 {
				reg = <8>;
				qcom,gpu-freq = <0>;
				qcom,bus-freq = <0>;
				qcom,bus-min = <0>;
				qcom,bus-max = <0>;
			};
		};
	};
};
